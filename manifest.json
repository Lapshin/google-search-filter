{
"name":"Google filter by Lapshin",
"description":"Remove freak sites from your searching results",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["*://www.google.com/search*"],
      "js": ["apply_filter.js"]
    }
  ],
  "options_ui": {
    "page":"options.html",
    "open_in_tab":false
  },
  "permissions": [
    "storage"
  ]
}